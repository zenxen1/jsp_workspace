<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<typeAliases>
<typeAlias type="com.fashion.product.domain.Product" alias="Product"/>
<typeAlias type="com.fashion.product.domain.TopCategory" alias="TopCategory"/>
<typeAlias type="com.fashion.product.domain.SubCategory" alias="SubCategory"/>
<typeAlias type="com.fashion.product.domain.Comments" alias="Comments"/>
<typeAlias type="com.fashion.member.domain.Member" alias="Member"/>
<typeAlias type="com.fashion.member.domain.Guest" alias="Guest"/>
<typeAlias type="com.fashion.shopping.domain.Bank" alias="Bank"/>
<typeAlias type="com.fashion.shopping.domain.OrderSum" alias="OrderSum"/>
<typeAlias type="com.fashion.shopping.domain.Customer" alias="Customer"/>
<typeAlias type="com.fashion.shopping.domain.OrderDetail" alias="OrderDetail"/>
<typeAlias type="com.fashion.shopping.domain.Payment" alias="Payment"/>
<typeAlias type="com.fashion.shopping.domain.Dest" alias="Dest"/>
<typeAlias type="com.fashion.product.domain.EventInfo" alias="EventInfo"/>
<typeAlias type="com.fashion.product.domain.EventProduct" alias="EventProduct"/>
<typeAlias type="com.fashion.emp.Dept" alias="Dept"/>
<typeAlias type="com.fashion.emp.Emp" alias="Emp"/>
</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="oracle.jdbc.driver.OracleDriver"/>
        <property name="url" value="jdbc:oracle:thin:@localhost:1521:XE"/>
        <property name="username" value="fashion"/>
        <property name="password" value="fashion"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/fashion/mybatis/ProductMapper.xml"/>
    <mapper resource="com/fashion/mybatis/TopcategoryMapper.xml"/>
    <mapper resource="com/fashion/mybatis/SubcategoryMapper.xml"/>
    <mapper resource="com/fashion/mybatis/CommentsMapper.xml"/>
    <mapper resource="com/fashion/mybatis/MemberMapper.xml"/>
     <mapper resource="com/fashion/mybatis/BankMapper.xml"/>
     <mapper resource="com/fashion/mybatis/OrderSumMapper.xml"/>
    <mapper resource="com/fashion/mybatis/GuestMapper.xml"/>
    <mapper resource="com/fashion/mybatis/PaymentMapper.xml"/>
     <mapper resource="com/fashion/mybatis/DestMapper.xml"/>
     <mapper resource="com/fashion/mybatis/EventInfoMapper.xml"/>
       <mapper resource="com/fashion/mybatis/EventProductMapper.xml"/>
        <mapper resource="com/fashion/mybatis/DeptMapper.xml"/>
  </mappers>
</configuration>